# Example .env file for slidedown configuration
# Copy this to .env and customize as needed

# Parser Configuration
# SLIDEDOWN_PLACEHOLDER_PREFIX=\x00CHILD_
# SLIDEDOWN_PLACEHOLDER_SUFFIX=\x00

# Compilation Configuration
# SLIDEDOWN_DEBUG_MODE=false
# SLIDEDOWN_VALIDATE_DIRECTIVES=true
# SLIDEDOWN_STRICT_MODE=false

# Asset Configuration
# SLIDEDOWN_DEFAULT_ASSETS_DIR=assets

# Output Configuration
# SLIDEDOWN_MINIFY_OUTPUT=false
